{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        { 
            "name": "client",
            "image": "fdsasfsdfds/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name":"server",
            "image": "fdsasfsdfds/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name":"worker",
            "image": "fdsasfsdfds/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image": "fdsasfsdfds/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]

}